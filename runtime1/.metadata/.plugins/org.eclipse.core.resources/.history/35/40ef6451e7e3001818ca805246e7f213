        <ownedRules xsi:type="validation:SemanticValidationRule" name="SsameNameResourcesUsageParameters" level="WARNING" message="aql:'The ResourcesUsage/Parameters &quot;'+self.name+'&quot; has the same name than another ResourcesUsage/Parameters'" targetClass="placer.Implementation">
          <audits auditExpression="service:sameNameResourcesUsageParameters()"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="SloopTask" level="ERROR" message="aql:'The Task &quot;'+self.name+'&quot; must not have a return on itself'" targetClass="placer.Task">
          <audits auditExpression="service:loopTask()"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="SconditionAlap" level="ERROR" message="aql:'It is impossible to have two transmissions &quot;Alap&quot; in input for a Task'" targetClass="placer.Transmission">
          <audits auditExpression="service:conditionAlap()"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="SconditionAsap" level="ERROR" message="aql:'It is impossible to have two transmissions &quot;Asap&quot; in output for a Task'" targetClass="placer.Transmission">
          <audits auditExpression="service:conditionAsap()"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="SvalidationImplementationResourcesProperties" level="ERROR" message="aql:'The Implementation &quot;'+self.name+'&quot; must have the resourcesUsage corresponding to its class'" targetClass="placer.Implementation">
          <audits auditExpression="service:validationImplementationResourcesProperties()"/>
        </ownedRules>
      </validationSet>
